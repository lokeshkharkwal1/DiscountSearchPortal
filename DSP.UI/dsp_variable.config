<DSP defaultBrandName="HT">
  <allBrands>
    <addBrand brandName="HT">
      <customError>
        <errorMessage value="Error has been occurred, Please contact System Administrator."/>
        <errorLogLocation value="D:/Log/HTLog"/>
      </customError>  
      <uiLogoAndBanner>
        <backgroundColor value="black !important" />
        <brandLogoPath value="~/Images/Logos/HT/logo.png" />
        <brandBannerPath value="~/Images/Logos/HT/banner.jpg" />
      </uiLogoAndBanner>
      <loginPage>
        <jobCodesAllowedToLogin value="KH,PTASM,FTASM,SM,DM,TEMPKH,TEMPASMP,TEMPASMF,TEMPSM" />
        <loginPageHeading value="Discount Eligibility" />
        <loginFieldLabel value="EMPLOYEE ID *" />
