        <loginButtonValue value="Login" />
        <errorMessageIfUserNotFound value="Invalid Employee Id" />
        <errorMessageIfUserNotAllowedToLogin value="You are not allowed to login. Use different Employee Id." />
        <range minValue="1" maxValue="999999" errorMessage="Please Enter value in between {0} to {1}" />
      </loginPage>
      <searchPage>
